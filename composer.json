{
  "name": "redfenix-eng/laravel-multiauth-multitenancy",
  "description": "Multi Auth in Laravel Project with support for the Laravel 8 Models namespace convention & optional generation inside a tenant when using the multitenancy stancl/tenancy package. Forked from bitfumes/laravel-multiauth",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel multiauth",
    "multitenancy",
    "multiauth",
    "bitfumes",
    "admin auth",
    "multi auth",
    "laravel permissions",
    "laravel roles permissions",
    "laravel acl"
  ],
  "authors": [
    {
      "name": "Toby Selway",
      "email": "tobyselway@redfenix.pt"
    },
    {
      "name": "sarthaksavvy",
      "email": "sarthak@bitfumes.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "laravel/ui": "^2.0|^3.0",
    "laravel/legacy-factories": "^1.0@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5|^9.3",
    "mockery/mockery": "^1.3.1",
    "orchestra/testbench": "^5.0|^6",
    "orchestra/database": "^5.0|^6",
    "illuminate/support": "7.0|8.0",
    "fzaninotto/faker": "^1.9"
  },
  "autoload": {
    "psr-4": {
      "Bitfumes\\Multiauth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bitfumes\\Multiauth\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bitfumes\\Multiauth\\MultiauthServiceProvider"
      ]
    }
  }
}
